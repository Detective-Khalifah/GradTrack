<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileOneVM" type="com.blogspot.thengnet.modules.profileone.data.viewmodel.ProfileOneVM"/>
    </data>
    <LinearLayout
        android:id="@+id/linearColumnmakeprofilepu"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:gravity="bottom"
        android:orientation="vertical">
                <LinearLayout android:id="@+id/linearEditImagePhot"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnNine"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylegray_8">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumncamera"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylegray_8">
                            <LinearLayout
                                android:id="@+id/linearRowcamera"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_11pxh"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_20pxv"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <ImageView android:id="@+id/imageCamera"
                                    android:layout_height="@dimen/_21pxh"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_width="@dimen/_25pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_camera"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_camera"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtInputtextSeventeen"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_17pxh"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_weight="0.87"
                                    android:layout_width="0dp"
                                    android:letterSpacing="0.03"
                                    android:lineSpacingExtra="@dimen/_4pxh"
                                    android:singleLine="true"
                                    android:text="@{profileOneVM.profileOneModel.txtInputtextSeventeen}"
                                    style="@style/txtRobotoregular18" tools:text="@string/msg_take_a_photo_us"/>
                                <ImageView
                                    android:id="@+id/imageArrowright"
                                    android:layout_height="@dimen/_21pxh"
                                    android:layout_marginBottom="@dimen/_2pxv"
                                    android:layout_marginStart="@dimen/_5pxh"
                                    android:layout_width="@dimen/_12pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_arrowright"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                            </LinearLayout>
                            <View android:background="@color/gray_902"
                                android:id="@+id/lineStrokeEighteen"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_357pxh"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnTen"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylegray_8">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnmail"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylegray_8">
                            <LinearLayout
                                android:id="@+id/linearRowarrowrightOne"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_12pxh"
                                android:layout_marginStart="@dimen/_12pxh"
                                android:layout_marginTop="@dimen/_20pxv"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <LinearLayout
                                    android:id="@+id/linearRowmail"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_weight="0.93"
                                    android:layout_width="0dp" android:orientation="horizontal">
                                    <ImageView
                                    android:id="@+id/imageMail"
                                    android:layout_height="@dimen/_19pxh"
                                    android:layout_marginBottom="@dimen/_2pxv"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_width="@dimen/_25pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_mail"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_mail"/>
                                    <TextView android:ellipsize="end"
                                    android:id="@+id/txtInputtextEighteen"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_weight="0.83"
                                    android:layout_width="0dp"
                                    android:letterSpacing="0.03"
                                    android:lineSpacingExtra="@dimen/_4pxh"
                                    android:singleLine="true"
                                    android:text="@{profileOneVM.profileOneModel.txtInputtextEighteen}"
                                    style="@style/txtRobotoregular18" tools:text="@string/lbl_upload_from"/>
                                </LinearLayout>
                                <ImageView
                                    android:id="@+id/imageArrowrightOne"
                                    android:layout_height="@dimen/_21pxh"
                                    android:layout_marginBottom="@dimen/_2pxv"
                                    android:layout_marginStart="@dimen/_144pxh"
                                    android:layout_width="@dimen/_12pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_arrowright"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                            </LinearLayout>
                            <View android:background="@color/gray_902"
                                android:id="@+id/lineStrokeNineteen"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_357pxh"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
    </LinearLayout>
</layout>
