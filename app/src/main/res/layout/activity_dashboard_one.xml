<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="dashboardOneVM" type="com.blogspot.thengnet.modules.dashboardone.data.viewmodel.DashboardOneVM"/>
    </data>
    <LinearLayout android:gravity="end"
        android:id="@+id/linearColumnback"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_9pxv"
        android:layout_marginTop="@dimen/_20pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" >
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearDashboardOne"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_width="match_parent"
            android:orientation="vertical">
            <LinearLayout android:gravity="end"
                android:id="@+id/linearGraduationTrac"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_100cornerRadius">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearViewdetails"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_6pxh"
                    android:layout_marginStart="@dimen/_270pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBack"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{dashboardOneVM.dashboardOneModel.txtBack}"
                        style="@style/txtRobotoromanregular14_1" tools:text="@string/lbl_back"/>
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_9pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_5pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_teal_200"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_teal_200"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAcademicYears"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_60pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linear1stYear"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.22"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageCircularprogre"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_25pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_circularprogre_30x25"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_circularprogre_30x25"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFirst"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{dashboardOneVM.dashboardOneModel.txtFirst}"
                            style="@style/txtRobotoromanregular10" tools:text="@string/lbl_1st"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linear2ndYear"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.22"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView
                            android:id="@+id/imageCircularprogreOne"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_25pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_circularprogre_1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_circularprogre_1"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSecond"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{dashboardOneVM.dashboardOneModel.txtSecond}"
                            style="@style/txtRobotoromanregular10" tools:text="@string/lbl_2nd"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linear3rdYear"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.22"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView
                            android:id="@+id/imageCircularprogreTwo"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_25pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_circularprogre_2"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_circularprogre_2"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtThird"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{dashboardOneVM.dashboardOneModel.txtThird}"
                            style="@style/txtRobotoromanregular10" tools:text="@string/lbl_3rd"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linear4thYear"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.22"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView
                            android:id="@+id/imageCircularprogreThree"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_25pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_circularprogre_3"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_circularprogre_3"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFourth"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{dashboardOneVM.dashboardOneModel.txtFourth}"
                            style="@style/txtRobotoromanregular10" tools:text="@string/lbl_4th"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linear5thYear"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:gravity="start"
                            android:id="@+id/imageCircularprogreFour"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_circularprogre_4"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_circularprogre_4"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtFifth"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_5pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{dashboardOneVM.dashboardOneModel.txtFifth}"
                            style="@style/txtRobotoromanregular10" tools:text="@string/lbl_5th"/>
                    </LinearLayout>
                </LinearLayout>
                <ProgressBar android:gravity="center_horizontal"
                    android:id="@+id/progressBarSliderprogress"
                    android:indeterminate="false"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:max="100" android:minHeight="@dimen/_9pxv"
                    android:progress="84"
                    android:progressDrawable="@drawable/layer_list_bg_gray_300_radius_4_5" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAcademicYears"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_48pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{dashboardOneVM.dashboardOneModel.txtAcademicYears}"
                    style="@style/txtRobotoromanregular14_1" tools:text="@string/lbl_academic_years"/>
            </LinearLayout>
        </LinearLayout>

        <FrameLayout
            android:id="@+id/frameBottombar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start">

                <LinearLayout
                    android:id="@+id/linearTabs"
                    style="@style/groupStylegreen_100_3f"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/linearMaterialYouTa"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageIcon"
                            circular="@{true}"
                            imageRes="@{@drawable/img_icon}"
                            android:layout_width="@dimen/_22pxh"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:scaleType="centerCrop"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_icon" />

                        <TextView
                            android:id="@+id/txtLabel"
                            style="@style/txtRobotomedium10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:letterSpacing="0.02"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{dashboardOneVM.dashboardOneModel.txtLabel}"
                            tools:text="@string/lbl_courses" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearMaterialYouTa1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <FrameLayout
                            android:id="@+id/frameIcon"
                            style="@style/groupStylegreen_3"
                            android:layout_width="@dimen/_55pxh"
                            android:layout_height="@dimen/_27pxv"
                            android:layout_gravity="center">

                            <ImageView
                                android:id="@+id/imageVectorThree"
                                android:layout_width="@dimen/_18pxh"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_marginBottom="@dimen/_5pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_vector" />
                        </FrameLayout>

                        <TextView
                            android:id="@+id/txtLabelOne"
                            style="@style/txtRobotomedium10_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:letterSpacing="0.02"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{dashboardOneVM.dashboardOneModel.txtLabelOne}"
                            tools:text="@string/lbl_dashboard" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearMaterialYouTa2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageUser"
                            circular="@{true}"
                            imageRes="@{@drawable/img_user_teal_200}"
                            android:layout_width="@dimen/_16pxh"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:scaleType="centerCrop"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_user_teal_200" />

                        <TextView
                            android:id="@+id/txtLabelTwo"
                            style="@style/txtRobotomedium10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:letterSpacing="0.02"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{dashboardOneVM.dashboardOneModel.txtLabelTwo}"
                            tools:text="@string/lbl_profile" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:id="@+id/lineHome"
                    android:layout_width="@dimen/_62pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:background="@drawable/rectangle_bg_gray_900" />
        </FrameLayout>
    </LinearLayout>
</layout>
