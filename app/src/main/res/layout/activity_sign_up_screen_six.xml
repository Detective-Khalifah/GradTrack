<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpScreenSixVM" type="com.blogspot.thengnet.modules.signupscreensix.data.viewmodel.SignUpScreenSixVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSignUpScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearCredentialsInp"
            android:layout_gravity="bottom|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_24pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnsupportingtext"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnlabeltext"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLabeltext"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.02"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:paddingBottom="@dimen/_1pxv"
                        android:paddingEnd="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{signUpScreenSixVM.signUpScreenSixModel.txtLabeltext}"
                        style="@style/txtSolid_2" tools:text="@string/lbl_phone_number"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtInputtext"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.03"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{signUpScreenSixVM.signUpScreenSixModel.txtInputtext}"
                        style="@style/txtRobotoregular16_1" tools:text="@string/lbl"/>
                </LinearLayout>
                <TextView android:id="@+id/txtSupportingtext"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.02"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:text="@{signUpScreenSixVM.signUpScreenSixModel.txtSupportingtext}"
                    style="@style/txtRobotoregular12_1" tools:text="@string/msg_if_this_isn_t_y2"/>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerListcreatefromfra"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_9pxh}" tools:itemCount="2" tools:listitem="@layout/row_listcreatefromfra2"/>
            <LinearLayout android:id="@+id/linearPrevNextButto"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_49pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnPrevious"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_width="@dimen/_104pxh"
                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_previous"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_previous"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSignUp"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginStart="@dimen/_104pxh"
                    android:layout_width="@dimen/_97pxh"
                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_sign_up"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_sign_up"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageUndrawbooksre"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_228pxh"
            android:layout_marginBottom="@dimen/_137pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_137pxv"
            android:layout_width="@dimen/_359pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_undrawbooksre"
            tools:ignore="ContentDescription" tools:src="@drawable/img_undrawbooksre"/>
        <TextView android:ellipsize="end" android:id="@+id/txtSignUpOne"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_53pxv"
            android:layout_marginEnd="@dimen/_47pxh"
            android:layout_marginStart="@dimen/_47pxh"
            android:layout_marginTop="@dimen/_53pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.02"
            android:lineSpacingExtra="@dimen/_7pxh"
            android:singleLine="true"
            android:text="@{signUpScreenSixVM.signUpScreenSixModel.txtSignUpOne}"
            style="@style/txtRobotoromancondensedblack36" tools:text="@string/lbl_sign_up"/>
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearTopappbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_A700">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginBottom="@dimen/_20pxv"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPartCounter"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_marginEnd="@dimen/_258pxh"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_weight="0.79"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{signUpScreenSixVM.signUpScreenSixModel.txtPartCounter}"
                        style="@style/txtRobotoromansemibold19" tools:text="@string/lbl_part_6"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <View android:background="@drawable/rectangle_bg_gray_900"
                android:id="@+id/lineHome"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_148pxh"
                android:layout_marginStart="@dimen/_148pxh"
                android:layout_marginTop="@dimen/_696pxv" android:layout_width="@dimen/_62pxh"/>
        </LinearLayout>
    </FrameLayout>
</layout>
