<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileZeroRowModel" type="com.blogspot.thengnet.modules.profilezero.data.model.ProfileZeroRowModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumn"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_101">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumninputtextTwelve"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_5">
            <LinearLayout android:id="@+id/linearRowinputtextTwelve"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_11pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtInputtextTwelve"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.72"
                    android:layout_width="0dp"
                    android:letterSpacing="0.03"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{profileZeroRowModel.txtInputtextTwelve}"
                    style="@style/txtRobotoregular16_2" tools:text="@string/lbl_nigerian"/>
                <ImageView android:id="@+id/imageUserOne"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_201pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_black_900_20x25"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_black_900_20x25"/>
            </LinearLayout>
            <View android:background="@color/gray_902"
                android:id="@+id/lineStrokeTwelve"
                android:layout_gravity="start"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_318pxh"/>
        </LinearLayout>
    </LinearLayout>
</layout>
