<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpScreenOneVM" type="com.blogspot.thengnet.modules.signupscreenone.data.viewmodel.SignUpScreenOneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSignUpScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnsignup"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_24pxv"
            android:layout_marginEnd="@dimen/_34pxh"
            android:layout_marginStart="@dimen/_35pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignUp"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_width="wrap_content"
                android:letterSpacing="0.02"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:singleLine="true"
                android:text="@{signUpScreenOneVM.signUpScreenOneModel.txtSignUp}"
                style="@style/txtRobotoromancondensedblack36" tools:text="@string/lbl_sign_up"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearBiodata1Input"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_154pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearField"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylecornerRadius_2">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnfloatinglabel"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_weight="0.96"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFloatingLabel"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:paddingEnd="@dimen/_4pxh"
                            android:paddingStart="@dimen/_4pxh"
                            android:singleLine="true"
                            android:text="@{signUpScreenOneVM.signUpScreenOneModel.txtFloatingLabel}"
                            style="@style/txtSolid_1" tools:text="@string/lbl_nationality"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowuser"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageUser"
                                android:layout_height="@dimen/_16pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_16pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_user"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtInput"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_20pxh"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.03"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@{signUpScreenOneVM.signUpScreenOneModel.txtInput}"
                                style="@style/txtRobotoregular16" tools:text="@string/lbl_country"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageIcon"
                        android:layout_height="@dimen/_5pxh"
                        android:layout_marginBottom="@dimen/_26pxv"
                        android:layout_marginEnd="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icon_gray_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_icon_gray_900"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearList"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A701cornerRadius">
                    <FrameLayout android:id="@+id/frameStackuserOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_64pxv"
                        android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_291pxh">
                        <LinearLayout android:id="@+id/linearListItem1"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageUserOne"
                                android:layout_height="@dimen/_16pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_16pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_user"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtItemCounter"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_20pxh"
                                android:layout_weight="0.71"
                                android:layout_width="0dp"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@{signUpScreenOneVM.signUpScreenOneModel.txtItemCounter}"
                                style="@style/txtGoogleSansTextregular16" tools:text="@string/lbl_item_1"/>
                        </LinearLayout>
                        <FrameLayout android:id="@+id/frameStackuserTwo"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_64pxv" android:layout_width="@dimen/_291pxh">
                            <LinearLayout
                                android:id="@+id/linearListItem2"
                                android:layout_gravity="bottom|start"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="match_parent"
                                android:orientation="horizontal" style="@style/groupStyledeep_purple_500_14">
                                <ImageView
                                    android:id="@+id/imageUserTwo"
                                    android:layout_height="@dimen/_16pxh"
                                    android:layout_marginBottom="@dimen/_8pxv"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_marginTop="@dimen/_8pxv"
                                    android:layout_width="@dimen/_16pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_user"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtItemCounterOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_6pxv"
                                    android:layout_marginEnd="@dimen/_197pxh"
                                    android:layout_marginStart="@dimen/_20pxh"
                                    android:layout_marginTop="@dimen/_6pxv"
                                    android:layout_weight="0.72"
                                    android:layout_width="0dp"
                                    android:lineSpacingExtra="@dimen/_3pxh"
                                    android:singleLine="true"
                                    android:text="@{signUpScreenOneVM.signUpScreenOneModel.txtItemCounterOne}"
                                    style="@style/txtGoogleSansTextregular16" tools:text="@string/lbl_item_2"/>
                            </LinearLayout>
                            <View
                                android:background="@drawable/rectangle_bg_bluegray_400_radius_2"
                                android:id="@+id/lineScrollbar"
                                android:layout_gravity="end"
                                android:layout_height="@dimen/_64pxv"
                                android:layout_marginEnd="@dimen/_4pxh"
                                android:layout_marginStart="@dimen/_10pxh" android:layout_width="@dimen/_4pxh"/>
                        </FrameLayout>
                    </FrameLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearListItem3"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageUserThree"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtItemCounterTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_weight="0.72"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{signUpScreenOneVM.signUpScreenOneModel.txtItemCounterTwo}"
                            style="@style/txtGoogleSansTextregular16" tools:text="@string/lbl_item_3"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearListItem4"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageUserFour"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtItemCounterThree"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_weight="0.72"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{signUpScreenOneVM.signUpScreenOneModel.txtItemCounterThree}"
                            style="@style/txtGoogleSansTextregular16" tools:text="@string/lbl_item_4"/>
                    </LinearLayout>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerListlabeltext"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_20pxh}" tools:itemCount="3" tools:listitem="@layout/row_listlabeltext1"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearPrevNextButton"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnPrevious"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_width="@dimen/_104pxh"
                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_previous"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_previous"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnNext"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginStart="@dimen/_104pxh"
                    android:layout_width="@dimen/_78pxh"
                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_next"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_next"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageUndrawbooksre"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_228pxh"
            android:layout_marginBottom="@dimen/_137pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_137pxv"
            android:layout_width="@dimen/_359pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_undrawbooksre"
            tools:ignore="ContentDescription" tools:src="@drawable/img_undrawbooksre"/>
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearTopappbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_A700">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginBottom="@dimen/_20pxv"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPartCounter"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_marginEnd="@dimen/_261pxh"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_weight="0.78"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{signUpScreenOneVM.signUpScreenOneModel.txtPartCounter}"
                        style="@style/txtRobotoromansemibold19" tools:text="@string/lbl_part_1"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <View android:background="@drawable/rectangle_bg_gray_900"
                android:id="@+id/lineHome"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_148pxh"
                android:layout_marginStart="@dimen/_148pxh"
                android:layout_marginTop="@dimen/_696pxv" android:layout_width="@dimen/_62pxh"/>
        </LinearLayout>
    </FrameLayout>
</layout>
