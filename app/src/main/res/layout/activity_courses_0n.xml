<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="courses0nVM" type="com.blogspot.thengnet.modules.courses0n.data.viewmodel.Courses0nVM"/>
    </data>
    <FrameLayout android:id="@+id/frameCourses0n"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <TextView android:gravity="top|start"
            android:id="@+id/txtDescription"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_32pxv"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="match_parent"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="@dimen/_5pxh"
            android:text="@{courses0nVM.courses0nModel.txtDescription}"
            style="@style/txtRobotomedium12" tools:text="@string/msg_lorem_ipsum_dol"/>
        <LinearLayout android:id="@+id/linearEnrolledCourse"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_299pxv"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_299pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtTextenrolledc"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.73" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{courses0nVM.courses0nModel.txtTextenrolledc}"
                style="@style/txtRobotoromansemibold16" tools:text="@string/msg_computer_progra"/>
            <LinearLayout android:id="@+id/linearRowstatistics"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginEnd="@dimen/_11pxh"
                android:layout_marginStart="@dimen/_78pxh"
                android:layout_weight="0.27" android:layout_width="0dp" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtStatistics"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{courses0nVM.courses0nModel.txtStatistics}"
                    style="@style/txtRobotoromanregular14_1" tools:text="@string/lbl_statistics"/>
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_5pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_teal_200"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_teal_200"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageCoursethumbnai"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_240pxh"
            android:layout_marginBottom="@dimen/_47pxv"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_47pxv"
            android:layout_width="@dimen/_320pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_20pxh}"
            imageRes="@{@drawable/img_coursethumbnai}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_coursethumbnai"/>
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_9pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_328pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
            </androidx.appcompat.widget.Toolbar>
            <View android:background="@drawable/rectangle_bg_gray_900"
                android:id="@+id/lineHome"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_148pxh"
                android:layout_marginStart="@dimen/_148pxh"
                android:layout_marginTop="@dimen/_696pxv" android:layout_width="@dimen/_62pxh"/>
        </LinearLayout>
    </FrameLayout>
</layout>
