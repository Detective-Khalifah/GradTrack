<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="dashboardZeroVM" type="com.blogspot.thengnet.modules.dashboardzero.data.viewmodel.DashboardZeroVM"/>
    </data>
    <LinearLayout android:gravity="end"
        android:id="@+id/linearColumnviewdetails"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_9pxv"
        android:layout_marginTop="@dimen/_20pxv"
        android:layout_width="match_parent"
        android:orientation="vertical">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearDashboardZero"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_width="match_parent"
            android:orientation="vertical">
            <LinearLayout android:id="@+id/linearGraduationTrac"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_100cornerRadius">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearViewdetails"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtViewDetails"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.93"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{dashboardZeroVM.dashboardZeroModel.txtViewDetails}"
                        style="@style/txtRobotoromanregular14_1" tools:text="@string/lbl_view_details"/>
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_9pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_5pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_teal_200"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_teal_200"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtTextgraduation"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{dashboardZeroVM.dashboardZeroModel.txtTextgraduation}"
                    style="@style/txtRobotoromanlight14" tools:text="@string/msg_you_re_doing_gr"/>
                <FrameLayout android:id="@+id/frameStackseventyfive"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_105pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_100pxh">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSeventyFive"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_40pxv"
                        android:layout_marginEnd="@dimen/_35pxh"
                        android:layout_marginStart="@dimen/_34pxh"
                        android:layout_marginTop="@dimen/_40pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{dashboardZeroVM.dashboardZeroModel.txtSeventyFive}"
                        style="@style/txtRobotoromanextrabold16" tools:text="@string/lbl_75"/>
                    <ImageView android:id="@+id/imageCircularprogre"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_105pxh"
                        android:layout_width="@dimen/_100pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_circularprogre}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_circularprogre"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCurrentSemeste"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{dashboardZeroVM.dashboardZeroModel.txtCurrentSemeste}"
                    style="@style/txtRobotoromanregular14_1" tools:text="@string/msg_current_semeste"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearEnrolledCourse"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_39pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTextenrolledc"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.58"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{dashboardZeroVM.dashboardZeroModel.txtTextenrolledc}"
                    style="@style/txtRobotoromansemibold16" tools:text="@string/msg_enrolled_course"/>
                <LinearLayout android:id="@+id/linearRowviewdetailsOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_138pxh"
                    android:layout_weight="0.42"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtViewDetailsOne"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.93"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{dashboardZeroVM.dashboardZeroModel.txtViewDetailsOne}"
                        style="@style/txtRobotoromanregular14_1" tools:text="@string/lbl_view_details"/>
                    <ImageView android:id="@+id/imageVectorOne"
                        android:layout_height="@dimen/_9pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_5pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_teal_200"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_teal_200"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="end"
                android:id="@+id/recyclerCoursesCards"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_8pxh}" tools:itemCount="4" tools:listitem="@layout/row_coursescards"/>
            <LinearLayout android:id="@+id/linearFailedCourses"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTextfailedcou"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.54"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{dashboardZeroVM.dashboardZeroModel.txtTextfailedcou}"
                    style="@style/txtRobotoromansemibold16" tools:text="@string/lbl_failed_courses"/>
                <LinearLayout android:id="@+id/linearRowviewdetailsTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_138pxh"
                    android:layout_weight="0.46"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtViewDetailsTwo"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.93"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{dashboardZeroVM.dashboardZeroModel.txtViewDetailsTwo}"
                        style="@style/txtRobotoromanregular14_1" tools:text="@string/lbl_view_details"/>
                    <ImageView android:id="@+id/imageVectorTwo"
                        android:layout_height="@dimen/_9pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_5pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_teal_200"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_teal_200"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="start"
                android:id="@+id/recyclerListinitial"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_4pxh}" tools:itemCount="4" tools:listitem="@layout/row_listinitial"/>
        </LinearLayout>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearColumnicon"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearTabs"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylegreen_100_3f">
                    <LinearLayout android:id="@+id/linearMaterialYouTa"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageIcon"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_22pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_icon}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icon"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtLabel"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.02"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{dashboardZeroVM.dashboardZeroModel.txtLabel}"
                            style="@style/txtRobotomedium10" tools:text="@string/lbl_courses"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearMaterialYouTa1"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <FrameLayout android:id="@+id/frameIcon"
                            android:layout_height="@dimen/_27pxv"
                            android:layout_width="@dimen/_55pxh" style="@style/groupStylegreen_3">
                            <ImageView
                                android:id="@+id/imageVectorThree"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_marginBottom="@dimen/_5pxv"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                        </FrameLayout>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtLabelOne"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.02"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{dashboardZeroVM.dashboardZeroModel.txtLabelOne}"
                            style="@style/txtRobotomedium10_1" tools:text="@string/lbl_dashboard"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearMaterialYouTa2"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageUser"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_user_teal_200}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_user_teal_200"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtLabelTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.02"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{dashboardZeroVM.dashboardZeroModel.txtLabelTwo}"
                            style="@style/txtRobotomedium10" tools:text="@string/lbl_profile"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
