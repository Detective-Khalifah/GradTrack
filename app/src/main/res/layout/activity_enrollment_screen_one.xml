<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="enrollmentScreenOneVM" type="com.blogspot.thengnet.modules.enrollmentscreenone.data.viewmodel.EnrollmentScreenOneVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearEnrollmentScre"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_1">
        <ImageView android:id="@+id/imageGroupSix"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_290pxh"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_63pxv"
            android:layout_width="@dimen/_359pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_group6_290x359"
            tools:ignore="ContentDescription" tools:src="@drawable/img_group6_290x359"/>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtYoureenrolled"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_4pxh"
            android:singleLine="true"
            android:text="@{enrollmentScreenOneVM.enrollmentScreenOneModel.txtYoureenrolled}"
            style="@style/txtRobotoregular22" tools:text="@string/msg_you_re_enrolled"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnSignUp"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_40pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="@dimen/_97pxh"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:paddingBottom="@dimen/_11pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh"
            android:paddingTop="@dimen/_11pxv" android:singleLine="true"
            android:text="@string/lbl_sign_up"
            android:textAllCaps="false" style="@style/btnSolidRounded_1" tools:text="@string/lbl_sign_up"/>
        <View android:background="@drawable/rectangle_bg_gray_900"
            android:id="@+id/lineHome"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginBottom="@dimen/_9pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_301pxv" android:layout_width="@dimen/_62pxh"/>
    </LinearLayout>
</layout>
