<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="forgotPasswordThreeVM" type="com.blogspot.thengnet.modules.forgotpasswordthree.data.viewmodel.ForgotPasswordThreeVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearForgotPassword"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:gravity="start"
            android:id="@+id/imageUndrawforgotp"
            android:layout_height="@dimen/_239pxv"
            android:layout_marginTop="@dimen/_74pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            android:src="@drawable/img_undrawbooksre"
            tools:ignore="ContentDescription" tools:src="@drawable/img_undrawbooksre"/>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtPasswordsucces"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_74pxh"
            android:layout_marginStart="@dimen/_74pxh"
            android:layout_marginTop="@dimen/_179pxv"
            android:layout_width="match_parent"
            android:letterSpacing="0.02"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:singleLine="true"
            android:text="@{forgotPasswordThreeVM.forgotPasswordThreeModel.txtPasswordsucces}"
            style="@style/txtRobotoromanregular16" tools:text="@string/msg_password_succes"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnSignIn"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_40pxv"
            android:layout_marginEnd="@dimen/_74pxh"
            android:layout_marginStart="@dimen/_74pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="@dimen/_92pxh"
            android:letterSpacing="0.01"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:paddingBottom="@dimen/_11pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh"
            android:paddingTop="@dimen/_11pxv" android:singleLine="true"
            android:text="@string/lbl_sign_in"
            android:textAllCaps="false" style="@style/btnSolidRounded" tools:text="@string/lbl_sign_in"/>
        <View android:background="@drawable/rectangle_bg_gray_900"
            android:id="@+id/lineHome"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginBottom="@dimen/_9pxv"
            android:layout_marginEnd="@dimen/_74pxh"
            android:layout_marginStart="@dimen/_74pxh"
            android:layout_marginTop="@dimen/_189pxv" android:layout_width="@dimen/_62pxh"/>
    </LinearLayout>
</layout>
